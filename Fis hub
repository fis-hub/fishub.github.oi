local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Fis Hub", HidePremium = false, SaveConfig = false, ConfigFolder = "Fis Hub"})


--Value

_G.AutoOpenGildedVoid = true

_G.AutoOpenGilded = true

_G.autofish = true

_G.autoOpenancient = true

_G.autoOpenlargetrove = true

_G.autoOpenlargevoid = true

_G.autoOpensmalltrove = true

_G.autoOpentrash = true
--function

function AutoOpenGilded()
    while _G.AutoOpenGilded == true do
        local args = {
            [1] = "OpenTrove",
            [2] = "{CB79CE58-18F9-4904-9C6D-CA4F24389A7D}",
            [3] = "GildedTrove",
            [4] = "None"
        }
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
        wait(.0000000000001)  
    end
end



function Autofish()
    while _G.autofish == true do
        local args = {
            [1] = "Fish",
            [2] = "{2C5A38EF-510E-4E61-B8E3-1491A8D9A0D5}"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
        wait(.00000000001)
    end 
end      


