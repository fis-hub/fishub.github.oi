--Troves



local Tab = Window:MakeTab({
	Name = "troves",
	Icon = "rbxassetid://18808599901",
	PremiumOnly = false
})



Tab:AddToggle({
	Name = "AutoOpen Gilded Trove",
	Default = false,
	Callback = function(Value)
		_G.AutoOpenGilded = Value
        AutoOpenGilded()
	end    
})


Tab:AddToggle({
	Name = "AutoOpen Ancient Tarove ",
	Default = false,
	Callback = function(Value)
		_G.autoOpenancient = Value
        autoOpenancient()
	end    
})

Tab:AddToggle({
	Name = "AutoOpen large Tarove ",
	Default = false,
	Callback = function(Value)
		_G.autoOpenlargetrove = Value
        autoOpenlargetrove()
	end    
})

Tab:AddToggle({
	Name = "AutoOpen gilded Void ",
	Default = false,
	Callback = function(Value)
		_G.AutoOpenGildedVoid = Value
        AutoOpenGildedVoid()
	end    
